    "release": "np"
  },
  "description": "Add file extensions to relative imports in TypeScript.",
  "dependencies": {
    "arg": "^5.0.2",
    "ts-morph": "^24.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.9.3",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "np": "^10.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vitest": "^2.1.5"
  }
}
