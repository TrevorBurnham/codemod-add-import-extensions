{
  "name": "codemod-add-import-extensions",
  "version": "0.1.2",
  "author": "Trevor Burnham",
  "homepage": "https://github.com/TrevorBurnham/codemod-add-import-extensions",
  "license": "MIT",
  "keywords": [
    "codemod"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
